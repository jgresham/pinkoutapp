<div class="content-section-b">

  <div class="container">

      <div class="row gallery-row">
        <div class="col-md-6 col">
          <div id="pink-fans" class="gal-img"></div>
        </div>
        <div class="col-md-6 col">
          <div id="with-fan" class="gal-img"></div>
        </div>
      </div>
      <div class="row gallery-row">
        <div class="col-md-6 col">
          <div id="block-o-ribbon" class="gal-img"></div>
        </div>
        <div class="col-md-6 col">
          <div id="cameron" class="gal-img"></div>
        </div>
      </div>

  </div>
  <!-- /.container -->

</div>
<!-- /.content-section-b -->


<div class="content-section-d">

  <div class="container">
      <div class="row info-row">
        <div class="col-md-4 col">
          <div id="" class="info-thumb thumbnail">
          <%= image_tag "one-eight.png", id: "one-eight", class:"info-img img-responsive", alt: "group" %>
          <div class="caption">
            <h3>One in eight women will be affected</h3>
          </div>
          </div>
        </div>
        <div class="col-md-4 col">
          <div class="info-thumb thumbnail">
            <div id="two-svg" class="info-img">
              <svg>
                <text id="second" x="50%" y="60%">
                  <tspan id="two">2</tspan>
                  <tspan id="nd">nd</tspan>
                </text>
              </svg>
            </div>
            <div class="caption">
              <h3>Second most harmful cancer in women</h3>
            </div>
          </div>
        </div>
        <div class="col-md-4 col">
          <div id="" class="info-thumb thumbnail">
            <%= image_tag "blue-ribbon.png", id: "blue-ribbon", class:"info-img img-responsive", alt: "group" %>
            <div class="caption">
              <h3>Men are often diagnosed at later stages</h3>
            </div>
          </div>
        </div>
      </div>

  </div>
  <!-- /.container -->

</div>

<div id="contact" class="content-section-c">

  <div class="container">
      <div class="row">
          <div class="col-lg-5 col-sm-6">
              <h3>Connect</h3>
              <hr class="section-heading-spacer">
              <div class="clearfix"></div>
              <div id="connect-icons">
                <a href="https://www.facebook.com/PinkOutAtOhioState">
                  <%= inline_svg('icons/facebook.svg', class: "connect-icon")%>
                </a>
                <a href="https://twitter.com/PinkOutOSU">
                  <%= inline_svg('icons/twitter.svg', class: "connect-icon")%>
                </a>
                <a href="https://instagram.com/pinkoutosu/">
                  <%= inline_svg('icons/instagram.svg', class: "connect-icon")%>
                </a>
                <a href="mailto:pinkoutatosu@gmail.com">
                  <%= inline_svg('icons/mail.svg', class: "connect-icon")%>
                </a>
              </div>
          </div>
          <div class="col-lg-5 col-lg-offset-2 col-sm-6">
              <h3>Sponsor</h3>
              <hr class="section-heading-spacer">
              <div class="clearfix"></div>
              <p class="lead">We're looking for partners who want to make a positive impact in the breast cancer community.<br></p>
              <span class="lead">Contact us at pinkoutatosu@gmail.com,</span><br>
              <span class="lead">or make a donation.</span><br><br>
                <input type="button" value="Donate" onclick="location.href='https://www.paypal.com/cgi-bin/webscr?cmd=_s-xclick&hosted_button_id=QPVGQKHNXWQAE';" name="donate" id="donate-button">
          </div>
      </div>

  </div>
  <!-- /.container -->

</div>
<!-- /.content-section-a -->

